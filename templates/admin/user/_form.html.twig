<div class="container">
    {{ form_start(form) }}
        {{ form_row(form.newemail, {'label':"Adresse mail"}) }}
        {{ form_row(form.newpassword, {'label':"Mot de passe"}) }}
        {{ form_row(form.Nom, {'label':"Nom"}) }}
        {{ form_row(form.Prenom, {'label':"Prénom"}) }}
        {{ form_row(form.Birthday) }}
        {{ form_row(form.phone, {'label':"Téléphone"}) }}
        <button class="btn btn-success">{{ button_label|default('Enregistrer') }}</button>
    {{ form_end(form) }}
</div>
